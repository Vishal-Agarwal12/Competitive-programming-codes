using namespace std;
bool check(ll mid, ll a[], ll n, ll time)
{
    ll currTime = 0;
    pqs pq;
    for (int i = 0; i < n; i++)
    {
        if (pq.size() == mid)
        {
            currTime = pq.top();
            pq.pop();
        }
        pq.push(currTime + a[i]);
    }

    while (pq.size())
    {
        currTime = pq.top();
        pq.pop();
    }
    return currTime <= time;
}
void solve()
{

    ll n, time;
    cin >> n >> time;

    ll a[n];
    fo(i, n) cin >> a[i];

    ll l = 1, r = n;
    while (l < r)
    {
        ll mid = (l + r) / 2;

        if (check(mid, a, n, time))
        {
            r = mid;
        }
        else
        {
            l = mid + 1;
        }
    }

    cout << l << endl;
}

signed main()
{
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    solve();

    return 0;
}
